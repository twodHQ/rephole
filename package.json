{
  "name": "rephole",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:api": "nest build api",
    "build:worker": "nest build background-worker",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:api": "nest start api",
    "start:worker": "nest start background-worker",
    "start:all": "concurrently \"npm run start:api\" \"npm run start:worker\"",
    "start:dev": "nest start --watch",
    "start:api:dev": "nest start api --watch",
    "start:api:dev:mem": "NODE_OPTIONS=\"--max-old-space-size=4096 --expose-gc\" nest start api --watch",
    "start:worker:dev": "nest start background-worker --watch",
    "start:worker:dev:mem": "NODE_OPTIONS=\"--max-old-space-size=4096 --expose-gc\" nest start background-worker --watch",
    "start:worker:dev:debug": "NODE_OPTIONS=\"--max-old-space-size=512 --expose-gc\" LOG_LEVEL=debug nest start background-worker --watch",
    "start:debug": "nest start --debug --watch",
    "start:api:debug": "nest start api --debug --watch",
    "start:worker:debug": "nest start background-worker --debug --watch",
    "start:prod": "node dist/apps/api/main",
    "start:prod:mem": "node --max-old-space-size=8192 --expose-gc dist/apps/api/main",
    "start:worker:prod": "node dist/apps/background-worker/main",
    "start:worker:prod:mem": "node --max-old-space-size=8192 --expose-gc dist/apps/background-worker/main",
    "lint": "eslint \"{apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.json --no-cache",
    "test:watch": "jest --config jest.config.json --watch",
    "test:cov": "jest --config jest.config.json --coverage",
    "test:debug": "node --config jest.config.json --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@bull-board/api": "^6.14.2",
    "@bull-board/express": "^6.14.2",
    "@bull-board/nestjs": "^6.14.2",
    "@chroma-core/default-embed": "^0.1.9",
    "@keyv/redis": "^5.1.4",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/typeorm": "^11.0.0",
    "bull": "^4.16.5",
    "bullmq": "^5.63.2",
    "cache-manager": "^7.2.5",
    "chromadb": "^3.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "csv-parse": "^6.1.0",
    "ioredis": "^5.8.2",
    "keyv": "^5.5.4",
    "mammoth": "^1.11.0",
    "multer": "^2.0.2",
    "nest-winston": "^1.10.2",
    "openai": "^6.9.1",
    "pdf-parse": "^2.4.5",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "simple-git": "^3.30.0",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "ulid": "^3.0.1",
    "web-tree-sitter": "^0.25.10",
    "winston": "^3.18.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
